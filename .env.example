APP_NAME=Teleprompt
APP_ENV=local
APP_KEY=
APP_DEBUG=true
APP_URL=http://localhost:8009

LOG_CHANNEL=stack

DB_CONNECTION=mysql
DB_HOST=mysql # The MySQL docker container has hostname 'mysql'
DB_PORT=3306
DB_DATABASE=teleprompt
DB_USERNAME=root
DB_PASSWORD=

BROADCAST_DRIVER=log
CACHE_DRIVER=file
QUEUE_CONNECTION=sync
SESSION_DRIVER=file
SESSION_LIFETIME=120

REDIS_HOST=127.0.0.1
REDIS_PASSWORD=null
REDIS_PORT=6379

MAIL_MAILER=smtp
MAIL_HOST=smtp.mailtrap.io
MAIL_PORT=2525
MAIL_USERNAME=null
MAIL_PASSWORD=null
MAIL_ENCRYPTION=null
MAIL_FROM_ADDRESS=null
MAIL_FROM_NAME="${APP_NAME}"

AWS_ACCESS_KEY_ID=
AWS_SECRET_ACCESS_KEY=
AWS_DEFAULT_REGION=us-east-1
AWS_BUCKET=

PUSHER_APP_ID=
PUSHER_APP_KEY=
PUSHER_APP_SECRET=
PUSHER_APP_CLUSTER=mt1

MIX_PUSHER_APP_KEY="${PUSHER_APP_KEY}"
MIX_PUSHER_APP_CLUSTER="${PUSHER_APP_CLUSTER}"

# The ports to expose from Docker to localhost
DOCKER_APP_PORT=8009
DOCKER_MIX_HOT_RELOAD_PORT=8090
DOCKER_MYSQL_PORT=33069

# The user ID for use in the container.
# Set to the output of `echo $UID`.
DOCKER_WWWUSER=1000

# Git settings inside the container
DOCKER_GIT_USER_NAME=""
DOCKER_GIT_USER_EMAIL=""
